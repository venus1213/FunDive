{
  "name": "@fundive/backend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "build": "prisma generate && tsc",
    "clean": "rm -rf dist",
    "dev": "prisma generate && tsx watch src/index.ts",
    "start": "prisma migrate deploy && node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "seed:admin": "tsx prisma/seed/admin.ts",
    "seed:bulk-users": "tsx prisma/seed/bulk-users.ts",
    "seed:bulk-users:ja": "tsx prisma/seed/bulk-users.ts 10 entrepreneur true true",
    "seed:bulk-users:en": "tsx prisma/seed/bulk-users.ts 10 entrepreneur true false",
    "generate:types": "prisma generate",
    "test-token": "NODE_OPTIONS='--loader ts-node/esm' node src/utils/test-token.ts",
    "postinstall": "corepack enable && corepack prepare pnpm@latest --activate && prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/s3-request-presigner": "^3.812.0",
    "@prisma/client": "5.22.0",
    "@tanstack/react-query": "^5.66.7",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@types/geoip-lite": "^1.4.4",
    "@types/multer": "^1.4.12",
    "@types/node-cron": "^3.0.11",
    "@types/stripe": "^8.0.417",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.1.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "react-hot-toast": "^2.5.1",
    "socket.io": "^4.8.1",
    "stripe": "^17.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20.17.16",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "pnpm": "^8.0.0",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "zod-prisma-types": "^3.2.4"
  }
}